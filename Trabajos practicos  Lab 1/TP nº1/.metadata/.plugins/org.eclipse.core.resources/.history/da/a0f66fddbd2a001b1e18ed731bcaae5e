#include <stdio.h>
#ifndef TP1_CALCULADORA_H_
#define TP1_CALCULADORA_H_

/* \brief Pide dos numeros y los suma
 * \param float primer numero.
 * \param float segundo numero.
 * \return float devuelve.
 */
float sumar (float, float);

/* \brief Pide dos numeros y los resta
 * \param float primer numero.
 * \param float segundo numero.
 * \return float devuelve.
 */
float restar (float, float);

/* \brief Pide dos numeros y los divide
 * \param float primer numero.
 * \param float segundo numero.
 * \return float devuelve.
 */
float dividir (float , float );

/* \brief Pide dos numeros y los multiplica
 * \param float primer numero.
 * \param float segundo numero.
 * \return float devuelve.
 */
float multilicar (float , float);

/* \brief Pide un numero y realiza el factorial
 * \param int
 * \return int devuelve.
 */
int factorial (int );

float ingresarNum (float numero);

#endif /* TP1_CALCULADORA_H_ */
